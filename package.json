{
  "name": "@manuscripts/body-editor",
  "description": "Prosemirror components for editing and viewing manuscripts",
  "version": "2.7.37-test1.0",
  "repository": "github:Atypon-OpenSource/manuscripts-body-editor",
  "license": "Apache-2.0",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build": "concurrently 'yarn:build:*'",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs --project tsconfig.build.json",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration --project tsconfig.build.json",
    "dev": "yarn dependencies-versions && concurrently 'yarn:build:* --watch'",
    "lint": "eslint src --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint src --fix --ext .ts,.tsx --max-warnings=0",
    "prebuild": "rimraf dist && yarn dependencies-versions",
    "dependencies-versions": "node versions.mjs > src/versions.ts",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "preversion": "concurrently 'yarn:typecheck' 'yarn:lint' 'yarn:test'",
    "pretest": "yarn prebuild",
    "test": "jest --runInBand",
    "typecheck": "tsc --noEmit",
    "version": "yarn build"
  },
  "dependencies": {
    "@iarna/word-count": "^1.1.2",
    "@manuscripts/json-schema": "2.2.11",
    "@manuscripts/library": "1.3.11",
    "@manuscripts/style-guide": "2.0.34",
    "@manuscripts/track-changes-plugin": "1.9.4",
    "@manuscripts/transform": "3.0.38-test1.0",
    "@popperjs/core": "^2.11.8",
    "astrocite-eutils": "^0.16.4",
    "codemirror": "^5.58.1",
    "dompurify": "^2.4.1",
    "formik": "^2.2.9",
    "grapheme-splitter": "^1.0.4",
    "history": "^4.10.1",
    "mathjax": "3.2.2",
    "oembed-providers": "^1.0.20241022",
    "prosemirror-collab": "^1.3.1",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-dropcursor": "^1.6.1",
    "prosemirror-gapcursor": "^1.3.1",
    "prosemirror-history": "^1.4.1",
    "prosemirror-inputrules": "^1.2.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.18.3",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-tables": "^1.5.0",
    "prosemirror-transform": "^1.7.0",
    "prosemirror-utils": "^0.9.6",
    "prosemirror-view": "^1.29.1",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-router-dom": "^6.25.0",
    "react-select": "^5.7.3",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@manuscripts/eslint-config": "^0.5.1",
    "@types/codemirror": "^5.60.5",
    "@types/dompurify": "^2.4.0",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-prosemirror": "^2.1.1",
    "lodash": "^4.17.21",
    "prettier": "^2.8.1",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^3.0.2",
    "text-encoding-polyfill": "^0.6.7",
    "typescript": "^4.0.5",
    "uuid": "^9.0.0"
  },
  "resolutions": {
    "@types/react": "^18.3.1",
    "prosemirror-tables": "^1.5.0"
  },
  "engines": {
    "node": ">=20.16.0"
  }
}